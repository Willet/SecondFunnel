commands:
 01_syncdb:
  command: "./manage.py syncdb --noinput"
  leader_only: true
 02_createadmin:
  command: "scripts/createadmin.py"
  leader_only: true
 03_collectstatic:
  command: "./manage.py collectstatic --noinput"
 04_migrate_schema:
  command: "./manage.py migrate --noinput"
  leader_only: true
 05_append_ssh_keys:
  command: "cat ./ssh_keys > /home/ec2-user/.ssh/authorized_keys"
 06_run_celery_worker:
  command: "./manage.py celery worker --loglevel=warning --logfile=celery_log --autoscale=10,3"
  leader_only: true
option_settings:
 "aws:elasticbeanstalk:container:python:environment":
  DJANGO_SETTINGS_MODULE: "secondfunnel.settings.production"
 "aws:elasticbeanstalk:container:python":
  WSGIPath: "secondfunnel/wsgi.py"
 "aws:elasticbeanstalk:container:python:staticfiles":
  "/static/": "static/"
